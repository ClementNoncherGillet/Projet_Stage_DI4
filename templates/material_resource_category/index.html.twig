{% extends 'base.html.twig' %}

{% block title %}MaterialResourceCategory index{% endblock %}

{% block body %}
    <h1>MaterialResourceCategory index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Categoryname</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for material_resource_category in material_resource_categories %}
            <tr>
                <td>{{ material_resource_category.id }}</td>
                <td>{{ material_resource_category.categoryname }}</td>
                <td>
                    <a href="{{ path('app_material_resource_category_show', {'id': material_resource_category.id}) }}">show</a>
                    <a href="{{ path('app_material_resource_category_edit', {'id': material_resource_category.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_material_resource_category_new') }}">Create new</a>
{% endblock %}
